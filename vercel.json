{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; img-src 'self' https://backend-lokkeroom-5y7575faz-nicolas17.vercel.app; script-src 'self' 'unsafe-inline'; script-src-elem 'self' 'unsafe-inline';"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/index.js"
        }
    ]
}