{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; script-src 'self' 'unsafe-inline';"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
        },
        {
            "src": "/redirect",
            "status": 308,
            "headers": {
                "Location": "https://www.google.com/"
            }
        },
        {
            "src": "/legacy",
            "status": 404
        }
    ]
}